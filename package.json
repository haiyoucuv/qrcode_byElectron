{
    "name": "get_qr",
    "version": "1.0.0",
    "description": "By haiyoucuv",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "win": "electron-builder --win --x64",
        "mac": "electron-builder --mac --x64",
        "linux": "electron-builder --linux --x64",
        "build": "electron-builder -mac -win -linux --x64"
    },
    "build": {
        "productName": "获得一个二维码",
        "copyright": "haiyoucuv",
        "appId": "cn.haiyoucuv.GetYourQR",
        "directories": {
            "output": "dist"
        },
        "mac": {
            "target": [
                "dmg",
                "zip"
            ],
            "icon": "icon.png"
        },
        "win": {
            "target": [
                "zip"
            ],
            "icon": "icon.png"
        }
    },
    "repository": "https://github.com/haiyoucuv/qrcode_byElectron.git",
    "keywords": [
        "Electron",
        "qrcode",
        "qr",
        "haiyoucuv"
    ],
    "author": "haiyoucuv",
    "license": "haiyoucuv",
    "devDependencies": {
        "electron": "^7.2.4",
        "electron-builder": "^21.2.0"
    },
    "dependencies": {
        "qr-image": "^3.2.0"
    }
}
